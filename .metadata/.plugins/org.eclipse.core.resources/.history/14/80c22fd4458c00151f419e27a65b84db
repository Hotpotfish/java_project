		System.out.println(gcd(4,2));
		
		
	}
	public static int KEC(int f, int d)
	{
		if(f <= d)
		{
			System.out.println("错误");
			return -1;
		}
		int x1,x2,x3,y1,y2,y3,q;
		while(y3 != 0 || y3 != 1)
		{
			
		}
	}
	public static int gcd(int a, int b)
	{
		int big,small;
		if(a >= b)
		{
			big  = a;
			small = b;
			
		}
		else
		{
			big = b;
			small = a;
		}
		int r = 0;
		int temp;
		while(r != 0)
		{
			r = big % small;
			big = small;	
			temp = small;
			small = r;
			r = temp;
			
		}
		return small;
		
		
	}

}
